program
  
 blocks
    define number fibonacci(number a)
        if (a < 2)
            return a;
        else
            return fibonacci(a - 1) + fibonacci(a - 2);
        endif
    enddefine
 endblocks


 start
   variables
        manynumbers xd[4, 3, 5];
        number a = 5;
   endvariables

   a = fibonacci(a);
   xd[5-4] = a;
 finish
 endprogram