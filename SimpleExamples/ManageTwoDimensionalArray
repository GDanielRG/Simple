program


variables
	
endvariables

blocks
	define manynumbers addArrayNumbers(manynumbers x, number a)
		
		variables
			number i = 1, j = 1;
			manynumbers arrayResult[5,5];
		endvariables

		while(i < (length*length + 1))
			while( j < lenght + 1)
				arrayResult[i,j] = x[i,j] + a;
				j = j + 1;
			endwhile
			j = 1;
		endwhile

		return arrayResult;

	enddefine

endblocks

